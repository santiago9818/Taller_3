# Taller_3

#include <stdio.h>
#include <stdlib.h>

int main()
{
   int x;// declaracion de variables

        printf("Tama√±o de la matriz: ");scanf("%i",&x);// instruccion para imprimir la matriz
        
        int a[x][x];// declaracion de arreglo
        int i , j, k ;// declaracion de variables
        for (i = 0; i < x; i++) {// ciclo filas
            for (j = 0; j < x; j++) {// ciclo columnas
                printf("Digite numero en la posicion [" , i , "][" , j , "]:");scanf("%i",&a[i][j]);// instruccion para digitar los numeros 
                
            }
        }
        for (i = 0; i < x; i++) {// ciclo filas
            for (j = 0; j < x; j++) {// ciclo columnas
                printf("%i", + a[i][j]);// impresion de la matriz
            }
            printf("\n");
        }
        printf("matriz transpuesta \n");// matriz transpuesta
        for (i = 0; i < x; i++) {// ciclo filas
            for (j = 0; j < x; j++) {// ciclo columnas
                printf("%i" , a[j][i]);// imrprecion de la transpuesta
            }
            printf("\n");
        }
  
 system("PAUSE");
  return 0;
}
